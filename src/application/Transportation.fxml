<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.TransportationController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="650.0" layoutX="2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@transportation%20background.png" />
