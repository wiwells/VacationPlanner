            <Image url="@Transportation.png" />
         </image>
      </ImageView>
      <TextField fx:id="costText" layoutX="14.0" layoutY="378.0" prefHeight="22.0" prefWidth="170.0" />
      <Button fx:id="submit" layoutX="213.0" layoutY="116.0" mnemonicParsing="false" onAction="#submitButton" text="Submit">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="home" layoutX="483.0" layoutY="53.0" mnemonicParsing="false" onAction="#sendHome" prefHeight="49.0" prefWidth="72.0" text="Home">
