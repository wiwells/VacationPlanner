         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <ComboBox fx:id="initialTravel" layoutX="14.0" layoutY="116.0" prefHeight="22.0" prefWidth="170.0" promptText="Select Transportation">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <Text layoutX="14.0" layoutY="108.0" strokeType="OUTSIDE" strokeWidth="0.0" text="How will you travel to Destination">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <Text layoutX="14.0" layoutY="219.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Means of Transportation during Travel">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <Button fx:id="submit2" layoutX="214.0" layoutY="228.0" mnemonicParsing="false" onAction="#submitButton2" text="Submit">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <ListView fx:id="meansList" layoutX="14.0" layoutY="264.0" prefHeight="22.0" prefWidth="276.0" />
      <ListView fx:id="moneyList" layoutX="14.0" layoutY="408.0" prefHeight="38.0" prefWidth="276.0" />
      <Button fx:id="submit3" layoutX="214.0" layoutY="328.0" mnemonicParsing="false" onAction="#submitButton3" text="Submit">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="clearbutton" layoutX="483.0" layoutY="136.0" mnemonicParsing="false" onAction="#clearProcess" prefHeight="49.0" prefWidth="72.0" text="Clear">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <ListView fx:id="infoView" layoutX="365.0" layoutY="239.0" prefHeight="200.0" prefWidth="200.0" />
