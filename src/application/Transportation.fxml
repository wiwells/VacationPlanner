            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="costname" layoutX="14.0" layoutY="309.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="170.0" text="Cost of Traveling?" />
